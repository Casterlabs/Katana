[
	{
		"name": "www",
		"port": 80,
		"ssl": {
			"enabled": false,
			"force": true,
			"allow_insecure": true,
			"certificate": "ssl/certificate.pem",
			"private_key": "ssl/private.key",
			"chain": "ssl/chain.pem"
		},
		"hosts": [
			{
				"type": "proxy",
				"hostnames": [
					"localhost"
				],
				"proxy_path": "/youtube/*",
				"include_path": true,
				"proxy_url": "https://www.youtube.com/oembed?format=json&url="
			},{
				"type": "static",
				"hostnames": [
					"localhost",
					"127.0.0.1"
				],
				"directory": "www",
				"require_file_extensions": false
			}
		]
	}
]